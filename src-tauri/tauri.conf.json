{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "pnpm run build",
    "beforeDevCommand": "pnpm run dev",
    "devPath": "http://localhost:5173",
    "distDir": "../build"
  },
  "package": {
    "productName": "Corgi AI",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "dialog": {
        "all": true,
        "ask": true,
        "confirm": true,
        "message": true,
        "open": true,
        "save": true
      },
      "path": {
        "all": true
      },
      "fs": {
        "all": true,
        "removeFile": true,
        "readDir": true,
        "scope": [
          "**"
        ]
      },
      "http": {
        "request": true,
        "scope": [
          "https://**",
          "http://**"
        ]
      },
      "protocol": {
        "asset": true,
        "assetScope": [
          "**"
        ]
      },
      "window": {
        "create": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "prabhu.corgi.app",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "api.openai.com",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": "*"
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Corgi AI by Prabhu Kiran Konda",
        "width": 800
      }
    ]
  }
}